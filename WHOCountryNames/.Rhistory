left_join(TableCases,by=c('ADM0NAME'='ctr')) %>%
mutate(CumulativeIncidence=round(TotalCases/Population*100000,1)) %>%
mutate(RateDeaths=round(TotalDeaths/Population*1000000,1))
return(SummaryTable_AllCountries)
}
